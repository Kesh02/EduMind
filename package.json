{
    "name": "edumind-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "clean": "turbo run clean",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,py}\"",
        "type-check": "turbo run type-check"
    },
    "devDependencies": {
        "turbo": "^1.10.16",
        "prettier": "^3.0.0",
        "@turbo/gen": "^1.10.16"
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@8.10.0"
}